import java.util.Scanner;
public class Eggs
{
   public static void main(String[] args)
   {
   
   Scanner in = new Scanner(System.in);
   System.out.println("Enter the number of eggs.");
   int numberOfEggs = in.nextInt();
   int eggsInDozens = eggsInDozens(numberOfEggs);
   System.out.printf("%d in dozens is %d." , numberOfEggs , eggsInDozens);
   
   }
   
   public static int eggsInDozens( int numberOfEggs )
   {
      int eggsInDozens = numberOfEggs / 12;
      return eggsInDozens;
   }
   
}